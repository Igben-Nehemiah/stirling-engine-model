   "source": [
    "import sympy as sp\n",
    "\n",
    "V_m, R, T, a, b = sp.symbols('V_m R T a b')\n",
    "P = sp.symbols('P')\n",
    "\n",
    "# Peng-Robinson equation of state\n",
    "P_eq = (R * T / (V_m - b)) - (a / (V_m * (V_m + b) + b * (V_m - b)))\n",
    "\n",
    "integral = sp.integrate(P_eq, V_m)\n",
    "\n",
    "integral_result = sp.simplify(integral)\n",
    "integral_result\n"
   ]
